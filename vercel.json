{
  "version": 2,
  "builds": [
    {
      "src": "api/cron.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/cron",
      "dest": "/api/cron.js",
      "methods": ["GET"]
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 12 * * 1-5"
    }
  ]
}